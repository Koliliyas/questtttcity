{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "class": "pythonjsonlogger.jsonlogger.JsonFormatter",
      "format": "%(asctime)s %(levelname)s %(name)s %(message)s %(filename)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "json"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["console"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": ["console"],
      "level": "INFO",
      "propagate": false
    },
    "": {
      "handlers": ["console"],
      "level": "INFO",
      "propagate": true
    }
  }
}
